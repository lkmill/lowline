{
  "rules": {
    "arrow-parens": [2, "always"],
    "func-names": 0,
    "no-cond-assign": 0,
    "no-console": [1, { "allow": ["warn", "error", "info"] }],
    "no-empty": [2, { "allowEmptyCatch": true }],
    "no-mixed-operators": 0,
    "no-multi-assign": 0,
    "no-param-reassign": 0,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true } ],
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0
  }
}
