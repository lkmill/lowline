{
  "name": "lowline",
  "version": "0.1.6",
  "description": "DOM library",
  "main": "lowline.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rimraf *.js es lib dist",
    "lint": "eslint src",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "BABEL_ENV=commonjs babel src --out-dir .",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "test": "BABEL_ENV=node NODE_ENV=testing node test | tap-spec",
    "test:main": "node test/test",
    "prepublish": "npm run clean && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lohfu/lowline.git"
  },
  "keywords": [
    "DOM"
  ],
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lohfu/lowline/issues"
  },
  "homepage": "https://github.com/lohfu/lowline#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "lodash": "^4.17.4",
    "qunit-extras": "^3.0.0",
    "rimraf": "^2.6.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {}
}
