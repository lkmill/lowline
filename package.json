{
  "name": "lowline",
  "version": "0.2.0",
  "description": "DOM library",
  "main": "node/index.js",
  "jsnext:main": "es/index.mjs",
  "module": "es/index.mjs",
  "browser": {
    "node/index.js": "./index.js"
  },
  "scripts": {
    "clean": "rimraf *.js es node",
    "lint": "eslint src",
    "build": "npm run build:cjs && npm run build:es && npm run build:node",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir .",
    "build:es": "BABEL_ENV=es babel src --keep-file-extension --out-dir es",
    "build:node": "BABEL_ENV=node babel src --out-dir node",
    "test": "BABEL_ENV=node NODE_ENV=testing node test | tap-spec",
    "test:main": "node test/test",
    "prepublish": "npm run clean && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lohfu/lowline.git"
  },
  "keywords": [
    "DOM"
  ],
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lohfu/lowline/issues"
  },
  "homepage": "https://github.com/lohfu/lowline#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-plugin-add-module-exports": "^0.2.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "lodash": "^4.17.10",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
