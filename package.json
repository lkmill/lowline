{
  "name": "lowline",
  "version": "0.3.0",
  "description": "DOM library",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "default": "./dist/*.cjs"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir dist --out-file-extension .cjs",
    "build:esm": "BABEL_ENV=esm babel src --out-dir dist",
    "build:types": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "test": "tape tests/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lohfu/lowline.git"
  },
  "keywords": [
    "DOM"
  ],
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lohfu/lowline/issues"
  },
  "homepage": "https://github.com/lohfu/lowline#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "tape": "lohfu/tape#esm-support",
    "typescript": "^4.3.2"
  },
  "dependencies": {}
}
